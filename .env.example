# Advent of Code session cookie
AOC_SESSION_COOKIE=your_session_cookie_here

# Advent of Code leaderboard ID
AOC_LEADERBOARD_ID=your_leaderboard_id_here

# Advent of Code leaderboard year
AOC_LEADERBOARD_YEAR=2024

# Discord webhook URL
DISCORD_WEBHOOK_URL=your_discord_webhook_url_here

# Base URL for Advent of Code
AOC_BASE_URL=https://adventofcode.com

# File to store message IDs
MESSAGE_STORE_FILE=discord_messages.json

# Update interval in seconds (default to 900 seconds)
UPDATE_INTERVAL=900